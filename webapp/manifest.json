{
  "_version": "1.17.0",
  "sap.app": {
    "id": "zbarcodescan",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.5",
      "toolsId": "96f8c017-73b8-4e42-99f8-f6ac0b7fb6e0"
    },
    "crossNavigation": {
      "inbounds": {
        "z_barcodescan-create": {
          "semanticObject": "z_barcodescan",
          "action": "create",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.71.59",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data.json"
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "zbarcodescan.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "zbarcodescan.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteViewBarcode",
          "pattern": ":?query:",
          "target": [
            "TargetViewBarcode"
          ]
        }
      ],
      "targets": {
        "TargetViewBarcode": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ViewBarcode",
          "viewName": "ViewBarcode"
        }
      }
    },
    "rootView": {
      "viewName": "zbarcodescan.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
