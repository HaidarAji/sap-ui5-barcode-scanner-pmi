{
    "_version": "1.17.0",
    "sap.app": {
        "id": "zbarcodescan",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.15.5",
            "toolsId": "96f8c017-73b8-4e42-99f8-f6ac0b7fb6e0"
        },
        "crossNavigation": {
            "inbounds": {
                "z_barcodescan-create": {
                    "semanticObject": "z_barcodescan",
                    "action": "create",
                    "title": "{{flpTitle}}",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            } 
        },
        "dataSources": {
            "ZGXXS_SCAN_BARCODE_SRV": {
                "uri": "/sap/opu/odata/sap/ZGXXS_SCAN_BARCODE_SRV",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }            
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.71.59",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "zbarcodescan.i18n.i18n"
                }
            },
            "": {
                "preload": true,
                "dataSource": "ZGXXS_SCAN_BARCODE_SRV",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            },
            
            "locationModel": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "useBatch": false
                },
                "dataSource": "ZGXXS_SCAN_BARCODE_SRV"
            },
            "headerModel": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "useBatch": false
                },
                "dataSource": "ZGXXS_SCAN_BARCODE_SRV"
            },
            "plantModel": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "useBatch": false
                },
                "dataSource": "ZGXXS_SCAN_BARCODE_SRV"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "zbarcodescan.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteViewBarcode",
                    "pattern": ":?query:",
                    "target": [
                        "TargetViewBarcode"
                    ]
                }
            ],
            "targets": {
                "TargetViewBarcode": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "ViewBarcode",
                    "viewName": "ViewBarcode"
                }
            }
        },
        "rootView": {
            "viewName": "zbarcodescan.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    }
}